<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en" target-language="es" original="ECHOGLOSSIAN/PROPERTIES/RESOURCES.RESX" tool-id="MultilingualAppToolkit" product-name="n/a" product-version="n/a" build-num="n/a">
    <header>
      <tool tool-id="MultilingualAppToolkit" tool-name="Multilingual App Toolkit" tool-version="4.0.6919.0" tool-company="Microsoft" />
    </header>
    <body>
      <group id="ECHOGLOSSIAN/PROPERTIES/RESOURCES.RESX" datatype="resx">
        <trans-unit id="ConfigTab1Name" translate="yes" xml:space="preserve">
          <source>Configuration</source>
          <target state="translated" state-qualifier="tm-suggestion">Configuración</target>
        </trans-unit>
        <trans-unit id="ConfigWindowTitle" translate="yes" xml:space="preserve">
          <source>Echoglossian Configuration</source>
          <target state="translated" state-qualifier="mt-suggestion">Configuración de Echoglossian</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Config Window title bar</note>
        </trans-unit>
        <trans-unit id="HelpMessage" translate="yes" xml:space="preserve">
          <source>Opens Echoglossian config window</source>
          <target state="translated" state-qualifier="mt-suggestion">Abre la ventana de configuración de Echoglossian</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Shown in plugin install window</note>
        </trans-unit>
        <trans-unit id="HoverTooltipIndicator" translate="yes" xml:space="preserve">
          <source>(?)</source>
          <target state="final" state-qualifier="mt-suggestion">(?)</target>
        </trans-unit>
        <trans-unit id="LanguageSelectionTooltip" translate="yes" xml:space="preserve">
          <source>Which language to translate to.</source>
          <target state="translated" state-qualifier="mt-suggestion">A qué idioma traducir.</target>
        </trans-unit>
        <trans-unit id="LanguageSelectLabelText" translate="yes" xml:space="preserve">
          <source>Language to translate to</source>
          <target state="translated" state-qualifier="mt-suggestion">Idioma al que traducir</target>
        </trans-unit>
        <trans-unit id="Overlay name" translate="yes" xml:space="preserve">
          <source>Battle talk translation</source>
          <target state="translated" state-qualifier="mt-suggestion">Traducción de charlas de batalla</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Translation overlay window name</note>
        </trans-unit>
        <trans-unit id="OverlayAdjustmentOrientations" translate="yes" xml:space="preserve">
          <source>Please adjust position if your overlay is not centered relative to talk popup.</source>
          <target state="translated" state-qualifier="mt-suggestion">Ajuste la posición si su superposición no está centrada en relación con la ventana emergente de conversación.</target>
        </trans-unit>
        <trans-unit id="OverlayPositionAdjustmentLabel" translate="yes" xml:space="preserve">
          <source>Position adjustment</source>
          <target state="translated" state-qualifier="mt-suggestion">Ajuste de posición</target>
        </trans-unit>
        <trans-unit id="OverlayToggleLabel" translate="yes" xml:space="preserve">
          <source>Display translated text overlay above game UI dialogue box instead of replacing its text with the translation</source>
          <target state="translated" state-qualifier="mt-suggestion">Mostrar la superposición con texto traducido sobre el cuadro de diálogo de la interfaz de usuario del juego en lugar de reemplazar su texto con la traducción</target>
        </trans-unit>
        <trans-unit id="OverlayWidthScrollLabel" translate="yes" xml:space="preserve">
          <source>Width multiplier</source>
          <target state="translated" state-qualifier="mt-suggestion">Multiplicador de ancho</target>
        </trans-unit>
        <trans-unit id="PatronButtonLabel" translate="yes" xml:space="preserve">
          <source>Buy lokinmodar a Coffee</source>
          <target state="translated" state-qualifier="mt-suggestion">Comprar lokinmodar a Coffee</target>
        </trans-unit>
        <trans-unit id="SaveCloseButtonLabel" translate="yes" xml:space="preserve">
          <source>Save and Close Config Window</source>
          <target state="translated" state-qualifier="mt-suggestion">Guardar y cerrar ventana de configuración</target>
        </trans-unit>
        <trans-unit id="TransLateBattletalkToggle" translate="yes" xml:space="preserve">
          <source>Translate battle talk</source>
          <target state="translated" state-qualifier="mt-suggestion">Traducir charla de batalla</target>
        </trans-unit>
        <trans-unit id="TranslateTalkToggleLabel" translate="yes" xml:space="preserve">
          <source>Translate talk</source>
          <target state="translated" state-qualifier="mt-suggestion">Traducir charla</target>
        </trans-unit>
        <trans-unit id="WaitingForTranslation" translate="yes" xml:space="preserve">
          <source>Awaiting translation...</source>
          <target state="translated" state-qualifier="mt-suggestion">A la espera de traducción...</target>
        </trans-unit>
      </group>
    </body>
  </file>
</xliff>