    <body>
      <group id="ECHOGLOSSIAN/PROPERTIES/RESOURCES.RESX" datatype="resx">
        <trans-unit id="ConfigTab1Name" translate="yes" xml:space="preserve">
          <source>Main Configuration</source>
          <target state="translated">Configurações principais</target>
        </trans-unit>
        <trans-unit id="ConfigWindowTitle" translate="yes" xml:space="preserve">
          <source>Echoglossian Configuration</source>
          <target state="translated" state-qualifier="mt-suggestion">Configuração do Echoglossian</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Config Window title bar</note>
        </trans-unit>
        <trans-unit id="HelpMessage" translate="yes" xml:space="preserve">
          <source>Opens Echoglossian config window</source>
          <target state="translated" state-qualifier="mt-suggestion">Abre janela de configuração do Echoglossian</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Shown in plugin install window</note>
        </trans-unit>
        <trans-unit id="HoverTooltipIndicator" translate="no" xml:space="preserve">
          <source>(?)</source>
          <target state="final" state-qualifier="mt-suggestion">(?)</target>
        </trans-unit>
        <trans-unit id="LanguageSelectionTooltip" translate="yes" xml:space="preserve">
          <source>Which language to translate to.</source>
          <target state="translated" state-qualifier="mt-suggestion">Para qual língua traduzir.</target>
        </trans-unit>
        <trans-unit id="LanguageSelectLabelText" translate="yes" xml:space="preserve">
          <source>Language to translate to</source>
          <target state="translated" state-qualifier="mt-suggestion">Linguagem para traduzir para</target>
        </trans-unit>
        <trans-unit id="Overlay name" translate="yes" xml:space="preserve">
          <source>Battle talk translation</source>
          <target state="translated" state-qualifier="mt-suggestion">Tradução de conversa de batalha</target>
          <note from="MultilingualBuild" annotates="source" priority="2">Translation overlay window name</note>
        </trans-unit>
        <trans-unit id="OverlayAdjustmentOrientations" translate="yes" xml:space="preserve">
          <source>Please adjust position if your overlay is not centered relative to the game's UI dialogue box.</source>
          <target state="translated">Ajuste a posição se a sobreposição não estiver centrada em relação à caixa de conversa da UI do jogo.</target>
        </trans-unit>
        <trans-unit id="OverlayPositionAdjustmentLabel" translate="yes" xml:space="preserve">
          <source>Position adjustment</source>
          <target state="translated" state-qualifier="mt-suggestion">Ajuste de posição</target>
        </trans-unit>
        <trans-unit id="OverlayToggleLabel" translate="yes" xml:space="preserve">
          <source>Display translated text overlay above game UI dialogue box instead of replacing its text with the translation</source>
          <target state="translated" state-qualifier="mt-suggestion">Exibir sobreposição com texto traduzido acima da caixa de diálogo de interface do jogo em vez de substituir seu texto pela tradução</target>
        </trans-unit>
        <trans-unit id="OverlayWidthScrollLabel" translate="yes" xml:space="preserve">
          <source>Dialogue Width multiplier</source>
          <target state="translated">Multiplicador de largura da sobreposição de tradução de diálogo</target>
        </trans-unit>
        <trans-unit id="PatronButtonLabel" translate="yes" xml:space="preserve">
          <source>Buy lokinmodar a Coffee</source>
          <target state="translated" state-qualifier="mt-suggestion">Comprar um café para lokinmodar</target>
        </trans-unit>
        <trans-unit id="SaveCloseButtonLabel" translate="yes" xml:space="preserve">
          <source>Save and Close Config Window</source>
          <target state="translated" state-qualifier="mt-suggestion">Salvar e fechar janela de Configuração</target>
        </trans-unit>
        <trans-unit id="TransLateBattletalkToggle" translate="yes" xml:space="preserve">
          <source>Translate battle talk</source>
          <target state="translated" state-qualifier="mt-suggestion">Traduzir conversas de batalha</target>
        </trans-unit>
        <trans-unit id="TranslateTalkToggleLabel" translate="yes" xml:space="preserve">
          <source>Translate talk</source>
          <target state="translated" state-qualifier="mt-suggestion">Traduzir conversas</target>
        </trans-unit>
        <trans-unit id="WaitingForTranslation" translate="yes" xml:space="preserve">
          <source>Awaiting translation...</source>
          <target state="translated" state-qualifier="mt-suggestion">Aguardando tradução...</target>
        </trans-unit>
        <trans-unit id="OverlayFontSizeLabel" translate="yes" xml:space="preserve">
          <source>Overlay Font Size</source>
          <target state="translated">Tamanho da fonte da sobreposição</target>
        </trans-unit>
        <trans-unit id="OverlayFontSizeOrientations" translate="yes" xml:space="preserve">
          <source>Select the font size for the dialogue text overlay</source>
          <target state="translated" state-qualifier="mt-suggestion">Selecione o tamanho da fonte para a sobreposição de texto de diálogo</target>
        </trans-unit>
        <trans-unit id="OverlayWidthMultiplierOrientations" translate="yes" xml:space="preserve">
          <source>Adjust translation overlay width</source>
          <target state="translated">Ajuste a largura da sobreposição de tradução</target>
        </trans-unit>
        <trans-unit id="SendPixButton" translate="yes" xml:space="preserve">
          <source>Send me a PIX donation!</source>
          <target state="translated" state-qualifier="mt-suggestion">Mande-me um PIX!</target>
        </trans-unit>
        <trans-unit id="WhatToTranslateText" translate="yes" xml:space="preserve">
          <source>Select what kind of text you want to be translated</source>
          <target state="translated">Selecione que tipo de texto deseja que seja traduzido</target>
        </trans-unit>
        <trans-unit id="Name" translate="no" xml:space="preserve">
          <source>Echoglossian</source>
          <target state="final">Echoglossian</target>
        </trans-unit>
        <trans-unit id="pix" translate="no" xml:space="preserve" extype="System.Resources.ResXFileRef, System.Windows.Forms">
          <source>..\Resources\pix.png;System.Byte[], mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089</source>
          <target state="final">..\Resources\pix.png;System.Byte[], mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089</target>
        </trans-unit>
        <trans-unit id="PixText" translate="yes" xml:space="preserve">
          <source>Scan the QR code to send me a PIX - only valid in Brazil</source>
          <target state="translated" state-qualifier="mt-suggestion">Escaneie o código QR para me enviar um PIX - válido apenas no Brasil</target>
        </trans-unit>
        <trans-unit id="CloseButtonLabel" translate="yes" xml:space="preserve">
          <source>Close</source>
          <target state="final" state-qualifier="tm-suggestion">Fechar</target>
        </trans-unit>
        <trans-unit id="PixQrWindowLabel" translate="yes" xml:space="preserve">
          <source>PIX qrcode</source>
          <target state="translated" state-qualifier="mt-suggestion">QRCODE PIX</target>
        </trans-unit>
        <trans-unit id="QRCodeInstructionsText" translate="yes" xml:space="preserve">
          <source>Scan the QR code to send me a PIX - only valid in Brazil</source>
          <target state="translated" state-qualifier="mt-suggestion">Escaneie o código QR para me enviar um PIX - válido apenas no Brasil</target>
        </trans-unit>
        <trans-unit id="TranslateToastToggleText" translate="yes" xml:space="preserve">
          <source>Translate in-game Toast Messages</source>
          <target state="translated">Traduzir mensagens Toast no jogo</target>
        </trans-unit>
        <trans-unit id="LangIdentError" translate="yes" xml:space="preserve">
          <source>The language could not be identified with an acceptable degree of certainty</source>
          <target state="translated">A linguagem não pôde ser identificada com um grau aceitável de certeza</target>
        </trans-unit>
        <trans-unit id="ConfigTab2Name" translate="yes" xml:space="preserve">
          <source>Toast configurations</source>
          <target state="translated">Configurações de Toasts</target>
        </trans-unit>
        <trans-unit id="ToastOverlayWidthMultiplierOrientations" translate="yes" xml:space="preserve">
          <source>Adjust toast translation overlay width</source>
          <target state="translated">Ajuste a largura de sobreposição de tradução de Toasts</target>
        </trans-unit>
        <trans-unit id="ToastOverlayWidthScrollLabel" translate="yes" xml:space="preserve">
          <source>Toast Overlay Width multiplier</source>
          <target state="translated">Multiplicador de largura para Toasts</target>
        </trans-unit>
        <trans-unit id="WhichToastsToTranslateText" translate="yes" xml:space="preserve">
          <source>Select which in-game Toast messages to translate</source>
          <target state="translated">Selecione quais mensagens Toast devem ser traduzidas</target>
        </trans-unit>
        <trans-unit id="FontColorSelectLabel" translate="yes" xml:space="preserve">
          <source>Overlay font color:</source>
          <target state="translated">Cor da fonte da sobreposição:</target>
        </trans-unit>
        <trans-unit id="OverlayColorSelectName" translate="no" xml:space="preserve">
          <source>OverlayColorSelect</source>
          <target state="final">OverlayColorSelect</target>
        </trans-unit>
        <trans-unit id="OverlayFontColorOrientations" translate="yes" xml:space="preserve">
          <source>Click to choose the overlay font color</source>
          <target state="translated">Clique para escolher a cor da fonte da sobreposição</target>
        </trans-unit>
        <trans-unit id="TranslateAreaToastToggleText" translate="yes" xml:space="preserve">
          <source>Translate in-game Area name Toast Messages</source>
          <target state="translated">Traduzir mensagens de nome de área</target>
        </trans-unit>
        <trans-unit id="TranslateClassChangeToastToggleText" translate="yes" xml:space="preserve">
          <source>Translate in-game Class/Job change Toast Messages</source>
          <target state="translated">Traduzir mensagens de troca de classe/job</target>
        </trans-unit>
        <trans-unit id="TranslateErrorToastToggleText" translate="yes" xml:space="preserve">
          <source>Translate in-game Error Toast Messages</source>
          <target state="translated">Traduzir mensagens de erro no jogo</target>
        </trans-unit>
        <trans-unit id="TranslateNpcNamesToggle" translate="yes" xml:space="preserve">
          <source>Translate NPCs names in dialogue boxes</source>
          <target state="translated" state-qualifier="mt-suggestion">Traduzir nomes de NPCs em caixas de diálogo</target>
        </trans-unit>
        <trans-unit id="TranslateQuestToastToggleText" translate="yes" xml:space="preserve">
          <source>Translate in-game Quest Toast Messages</source>
          <target state="translated">Traduzir mensagens de quest</target>
        </trans-unit>
        <trans-unit id="TranslateScreenInfoToastToggleText" translate="yes" xml:space="preserve">
          <source>Translate other in-game miscellaneous Toast Messages</source>
          <target state="translated">Traduzir no jogo outras mensagens de Toast diversas</target>
        </trans-unit>
